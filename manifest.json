{
    "manifest_version": 2,
    "name": "ToDo",
    "description": "__MSG_description__",
    "version": "3.1",
    "default_locale": "en",
    "icons": {
        "16": "icons/16.png",
        "32": "icons/32.png",
        "48": "icons/48.png",
        "128": "icons/128.png"
    },
    "browser_action": {
        "default_popup": "ui/popup.html"
    },
    "options_page": "ui/options.html",
    "optional_permissions": [
        "downloads"
    ],
    "offline_enabled": true,
    "permissions": [
        "storage"
    ]
}
